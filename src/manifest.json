{
	"manifest_version": 2,
	"name": "Bookmarks Startpage",
	"description": "This is a startpage that uses chrome bookmarks.",
	"homepage_url": "https://github.com/rpbritton/bookmarks-startpage/",
	"version": "1.0",
	"incognito": "split",
	"chrome_url_overrides": {
		"newtab": "index.html"
	},
	"permissions": [
		"storage",
		"contextMenus",
		"bookmarks",
		"*://talaikis.com/api/quotes/*"
	],
	"icons": {
		"128": "img/icon.png"
	},
	"background": {
		"persistant": false,
		"scripts": ["js/background.js"]
	}
}
