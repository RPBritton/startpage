{
	"manifest_version": 2,

	"name": "bookmarks startpage",
	"description": "this is a startpage that uses bookmarks",
	"version": "1.0",
	"incognito": "split",
	"chrome_url_overrides": {
		"newtab": "startpage.html"
	},
	"permissions": [
	"bookmarks"
	],
	"browser_action": {
		"default_title": "bookmarks startpage",
		"default_icon": "icon.png"
	}
}
